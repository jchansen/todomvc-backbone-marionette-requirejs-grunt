<!doctype html>
<html lang="en" data-framework="marionettejs">

<head>
  <meta charset="utf-8">
  <title>Backbone.Marionette & Requirejs â€¢ TodoMVC</title>
  <link rel="stylesheet" href="stylesheets/base.css">
</head>

<body>

  <section class="todoapp">

    <div id="bannerRegion"></div>
    <div id="notepadRegion"></div>
    <div id="filterRegion"></div>

    <footer class="filter">
      <span class="todo-count"><div></div></span>
      <ul class="filters">
        <li><a href="#/">All</a></li>
        <li><a href="#/active">Active</a></li>
        <li><a href="#/completed">Completed</a></li>
      </ul>
      <button class="clear-completed">
        <div></div>
      </button>
    </footer>

    <footer class="filter">
      <span class="todo-count"><strong>2</strong> items left</span>
      <ul class="filters">
        <li><a href="#/">All</a></li>
        <li><a href="#/active">Active</a></li>
        <li><a href="#/completed">Completed</a></li>
      </ul>
      <button class="clear-completed" style="display: none;">Clear completed (0)</button>
    </footer>

    <footer class="filter">
      <span class="todo-count"><strong>2</strong> items left</span>
      <ul class="filters">
        <li><a href="#/">All</a></li>
        <li><a href="#/active">Active</a></li>
        <li><a href="#/completed">Completed</a></li>
      </ul>
      <button class="clear-completed" style="display: none;">Clear completed (0)</button>
    </footer>

    <div id="infoRegion"></div>

  </section>

  <script data-main="app/main" src="bower_components/requirejs/require.js"></script>
</body>

</html>
